extends ../includes/layouts/app-layoutblock content  .row    .col-lg      .card        .card-body.pt-3          ul.nav.nav-tabs.nav-tabs-bordered            li.nav-item              button.nav-link.active(data-bs-toggle='tab' data-bs-target='#myBalance')=wallets[0].name            li.nav-item              button.nav-link(data-bs-toggle='tab' data-bs-target='#etherum')=wallets[1].name            li.nav-item              button.nav-link(data-bs-toggle='tab' data-bs-target='#shibaInu')=wallets[2].name            li.nav-item              button.nav-link(data-bs-toggle='tab' data-bs-target='#safeMoon')=wallets[3].name          .tab-content.pt-2            include ../includes/components/wallets/my-balance            include ../includes/components/wallets/ethereum            include ../includes/components/wallets/shiba-inu            include ../includes/components/wallets/safemoonblock scripts  script.    var wallets = !{JSON.stringify(wallets)}  script(src='/js/wallets.js')